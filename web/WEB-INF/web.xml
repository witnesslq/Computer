<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>FklAction</servlet-name>
        <servlet-class>fkl.action.FklAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SalAction</servlet-name>
        <servlet-class>sal.action.SalAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SunAction</servlet-name>
        <servlet-class>sun.action.SunAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReleAction</servlet-name>
        <servlet-class>rele.action.ReleAction</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FklAction</servlet-name>
        <url-pattern>/FklAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SalAction</servlet-name>
        <url-pattern>/SalAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SunAction</servlet-name>
        <url-pattern>/SunAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReleAction</servlet-name>
        <url-pattern>/ReleAction</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>/jsp/index.html</welcome-file>
    </welcome-file-list>
</web-app>